{
  "manifest_version": 3,
  "name": "Save URL Extension",
  "version": "1.0",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo128.png"
  },
  "icons": {
    "128": "logo128.png",
    "64": "logo64.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
